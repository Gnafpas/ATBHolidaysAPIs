<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Beans.ATBDBBeans.KalitaonHotel.RoombedBean" table="roombed" schema="Sunhotels">
        <id name="id" column="id"/>
        <property name="bedCount" column="bed_count"/>
        <property name="bedType" column="bed_type"/>
        <property name="hotelId" column="hotel_id"/>
        <property name="roomId" column="room_id"/>
        <property name="extraBedCount" column="extra_bed_count"/>
        <property name="providerId" column="provider_id"/>
    </class>
</hibernate-mapping>