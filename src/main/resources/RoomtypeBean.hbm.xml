<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Beans.ATBDBBeans.KalitaonHotel.RoomtypeBean" table="roomtype" schema="Sunhotels">
        <id name="id" column="id"/>
        <property name="roomType" column="room_type"/>
        <property name="providerId" column="provider_id"/>
        <property name="sharedRoom" column="shared_room"/>
        <property name="roomtypeId" column="roomtype_id"/>
        <property name="description" column="description"/>
        <property name="roomSize" column="room_size"/>
        <property name="sizeMeasurement" column="size_measurement"/>
        <property name="minAdultOccupation" column="min_adult_occupation"/>
        <property name="maxAdultOccupation" column="max_adult_occupation"/>
        <property name="minChildOccupation" column="min_child_occupation"/>
        <property name="maxChildOccupation" column="max_child_occupation"/>
        <property name="minInfantOccupation" column="min_infant_occupation"/>
        <property name="maxInfantOccupation" column="max_infant_occupation"/>
    </class>
</hibernate-mapping>