<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_Beans.ViatorDestinationsBean" table="viator_destinations" schema="develope_viator">
        <id name="rid" column="rid"/>
        <property name="destinationId" column="destination_id"/>
        <property name="parentId" column="parent_id"/>
        <property name="lookupId" column="lookup_id"/>
        <property name="destinationType" column="destination_type"/>
        <property name="destinationNameEn" column="destination_name_EN"/>
        <property name="latitude" column="latitude"/>
        <property name="longitude" column="longitude"/>
        <property name="timeZone" column="time_zone"/>
        <property name="iataCode" column="iata_code"/>
        <property name="defaultCurrencyCode" column="default_currency_code"/>
        <property name="selectable" column="selectable"/>
        <property name="sortOrder" column="sort_order"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
        <property name="destinationUrlName" column="destination_Url_Name"/>
    </class>
</hibernate-mapping>