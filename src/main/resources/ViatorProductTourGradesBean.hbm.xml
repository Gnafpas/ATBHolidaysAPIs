<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_Beans.ViatorProductTourGradesBean" table="viator_product_tour_grades" schema="develope_viator">
        <id name="rid" column="rid"/>
        <property name="productCode" column="product_code"/>
        <property name="gradeCode" column="grade_code"/>
        <property name="gradeTitle" column="grade_title"/>
        <property name="gradeDepartureTime" column="grade_departure_time"/>
        <property name="gradeDescription" column="grade_description"/>
        <property name="defaultLanguageCode" column="default_language_code"/>
        <property name="currencyCode" column="currency_code"/>
        <property name="priceFrom" column="price_from"/>
        <property name="merchantNetPriceFrom" column="merchant_net_price_from"/>
        <property name="sortOrder" column="sort_order"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>