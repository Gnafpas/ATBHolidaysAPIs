<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DBBeans.ViatorAvailabilityDatesBean" table="viator_availability_dates" schema="develope_viator">
        <id name="rid" column="rid"/>
        <property name="productCode" column="product_code"/>
        <property name="year" column="year"/>
        <property name="month" column="month"/>
        <property name="day" column="day"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>