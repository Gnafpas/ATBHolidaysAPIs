<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DBBeans.ViatorProductAgeBandsBean" table="viator_product_age_bands" schema="develope_viator">
        <id name="rid" column="rid"/>
        <property name="productCode" column="product_code"/>
        <property name="bandId" column="band_id"/>
        <property name="ageFrom" column="age_from"/>
        <property name="ageTo" column="age_to"/>
        <property name="adult" column="adult"/>
        <property name="treatAsAdult" column="treat_as_adult"/>
        <property name="descriptionEn" column="description_EN"/>
        <property name="pluralDescriptionEn" column="plural_description_EN"/>
        <property name="count" column="count"/>
        <property name="sortOrder" column="sort_order"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>