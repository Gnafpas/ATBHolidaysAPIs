<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Beans.ATBDBBeans.KalitaonProduct.DProductPhotoBean" table="d_product_photo" schema="atb_DB">
        <id name="id" column="id"/>
        <property name="productId" column="product_id"/>
        <property name="subId" column="sub_id"/>
        <property name="vt" column="vt"/>
        <property name="photoName" column="photo_name"/>
        <property name="caption" column="caption"/>
        <property name="mainPhoto" column="main_photo"/>
        <property name="supplierId" column="supplier_id"/>
        <property name="atb" column="atb"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>