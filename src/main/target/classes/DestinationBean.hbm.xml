<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Beans.ATBDBBeans.KalitaonHotel.DestinationBean" table="destination" schema="Sunhotels">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="destinationId" column="destination_id"/>
        <property name="hotelBedsCode" column="hotel_beds_code"/>
        <property name="providerRef" column="provider_ref"/>
        <property name="countryName" column="country_name"/>
        <property name="countryCode" column="country_code"/>
        <property name="timezone" column="timezone"/>
        <property name="providerId" column="provider_id"/>
        <property name="sortOrder" column="sort_order"/>
    </class>
</hibernate-mapping>