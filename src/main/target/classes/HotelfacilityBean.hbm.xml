<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Beans.ATBDBBeans.KalitaonHotel.HotelfacilityBean" table="hotelfacility" schema="Sunhotels">
        <id name="id" column="id"/>
        <property name="price" column="price"/>
        <property name="explanation" column="explanation"/>
        <property name="facilityId" column="facility_id"/>
        <property name="hotelId" column="hotel_id"/>
        <property name="providerId" column="provider_id"/>
    </class>
</hibernate-mapping>